{
  "name": "music-api",
  "version": "1.0.0",
  "description": "Rest API using Node.js, TypeScript, Express",
  "main": "server.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "tsc",
    "start": "NODE_ENV=production node ./dist/src/server.js",
    "runprod": "NODE_ENV=production tsc && node ./dist/src/server.js",
    "dev": "nodemon",
    "test": "NODE_ENV=test jest --runInBand",
    "watch-test": "npm run test -- --watchAll",
    "db-migrate": "npx sequelize db:create && npx sequelize db:migrate",
    "db-seed": "npx sequelize db:seed:all",
    "db-test-migrate": "NODE_ENV=test npm run db-migrate",
    "db-test-seed": "NODE_ENV=test npm run db-seed",
    "db-create": "tsc && npm run db-migrate && npm run db-test-migrate && npm run db-seed && npm run db-test-seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/compression": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "jest": "^29.7.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.1",
    "sequelize": "^6.36.0",
    "ts-jest": "^29.1.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  }
}